#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
	//code
	int t;
    cin>>t;
    while(t--)
    {
        int m,n;
        cin>>m>>n;
        unordered_set<int> first;
        for(int i=0;i<m;i++)
        {
            int ele;
            cin>>ele;
            first.insert(ele);
        }
        for(int i=0;i<n;i++)
        {
            int ele;
            cin>>ele;
            first.insert(ele);
            
        }
        cout<<first.size()<<endl;
    }
	return 0;
}