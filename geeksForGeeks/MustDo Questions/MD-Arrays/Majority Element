//  https://practice.geeksforgeeks.org/problems/majority-element/0/?track=md-arrays&batchId=144

//   Mooreâ€™s Voting Algorithm

#include <iostream>
using namespace std;

int main() 
{
    int t;
    cin>>t;
    for(int k=0;k<t;k++)
    {
        int n;
        cin>>n;
        int arr[n];
        for(int i=0;i<n;i++)
        {
            cin>>arr[i];
        }
        int maj_index=0,count=1;
        for(int i=1;i<n;i++)
        {
            if(arr[maj_index] == arr[i])
            {
                count++;
            }
            else
                count--;
            if(count == 0)
            {
                count=1;
                maj_index=i;
            }
        }
        int maj_element=arr[maj_index];
        count=0;
        for(int i=0;i<n;i++)
        {
            if(arr[i] == maj_element)
            {
                count++;
            }
        }
        if(count > n/2)
        {
            cout<<maj_element<<endl;
        }
        else
            cout<<"-1"<<endl;
        
    }
	//code
	return 0;
}