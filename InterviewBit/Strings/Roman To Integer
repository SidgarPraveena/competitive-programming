int Solution::romanToInt(string A) 
{
    map<char, int> mp;
    mp['I'] = 1;
    mp['V'] = 5;
    mp['X'] = 10;
    mp['L'] = 50;
    mp['C'] = 100;
    mp['D'] = 500;
    mp['M'] = 1000; 
    int res = 0;
    int n = A.size()-1;
    res = res + mp[A[n]];
    for(int i = n-1; i >= 0; i--)
    {
        if(mp[A[i]] >= mp[A[i+1]])
        {
            res += mp[A[i]];
        }
        else
            res -= mp[A[i]];
    }
    return res;
}
    