ListNode* Solution::swapPairs(ListNode* head) 
{
    if(head == NULL || head->next == NULL)
        return head;
    ListNode *curr=head->next->next;
    ListNode *prev=head;
    head=head->next;
    head->next=prev;
    
    while(curr != NULL && curr->next != NULL)
    {
        prev->next = curr->next;
        prev=curr;
        ListNode * N=curr->next->next;
        curr->next->next=curr;
        curr=N;
    }
    prev->next=curr;
    return head;
}